{
  "name": "rubberband",
  "version": "0.2.1",
  "description": "Lightweight security assessment tool for OpenClaw installations",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "rubberband": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "prepublishOnly": "npm run lint && npm run test && npm run build"
  },
  "keywords": [
    "openclaw",
    "moltbot",
    "clawdbot",
    "security",
    "audit",
    "ai-agent"
  ],
  "author": "Gervi Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gervilabs/rubberband.git"
  },
  "bugs": {
    "url": "https://github.com/gervilabs/rubberband/issues"
  },
  "homepage": "https://github.com/gervilabs/rubberband#readme",
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "chalk": "^5.3.0",
    "json5": "^2.2.3",
    "diff": "^5.2.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/diff": "^5.0.9",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.0",
    "typescript": "^5.4.0",
    "vitest": "^1.3.0"
  }
}
